<Window x:Class="MUHITAV_Autoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MUHITAV_Autoservice"
        mc:Ignorable="d"
        Title="Автосервис Мухитова" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="/Resources/service_logo.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Услуги автосервиса" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name ="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Content="Назад" Name ="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin ="5"
                Click ="BtnBack_Click"></Button>
    </Grid>
</Window>
